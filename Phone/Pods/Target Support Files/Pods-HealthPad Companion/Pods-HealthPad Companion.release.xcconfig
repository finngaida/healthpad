EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/ActionKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Async.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwitchLoader.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "ActionKit" -framework "Async" -framework "SwitchLoader"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = "$(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-HealthPad Companion"
PODS_ROOT = ${SRCROOT}/Pods
